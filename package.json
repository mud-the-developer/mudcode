{
  "name": "@mudramo/mudcode",
  "version": "1.1.3",
  "description": "Bridge AI agent outputs to Discord via Claude Code hooks and tmux",
  "type": "module",
  "main": "./dist/index.js",
  "bin": {
    "mudcode": "bin/mudcode"
  },
  "scripts": {
    "dev": "tsx src/daemon-entry.ts",
    "build": "tsup",
    "build:watch": "tsup --watch",
    "prepare": "npm run build",
    "start": "node dist/src/daemon-entry.js",
    "pages:dev": "vite",
    "pages:build": "vite build",
    "pages:preview": "vite preview",
    "pages:deploy": "vite build && wrangler pages deploy ./dist-site --project-name=mudcode-landing",
    "setup:claude-plugin": "tsx scripts/setup-claude-plugin.ts",
    "setup:gemini-hook": "tsx scripts/setup-gemini-hook.ts",
    "setup:opencode-plugin": "tsx scripts/setup-opencode-plugin.ts",
    "migration:check": "node scripts/migrate-discode-cleanup.mjs --check",
    "migration:clean": "node scripts/migrate-discode-cleanup.mjs --clean",
    "build:release:binaries": "bun run scripts/build-binaries.ts",
    "build:release:binaries:single": "bun run scripts/build-binaries.ts --single",
    "build:release:binaries:host": "bun run scripts/build-binaries.ts --host-only",
    "build:release:binaries:linux": "bun run scripts/build-binaries.ts --profile linux",
    "build:release:manifest": "node scripts/rebuild-release-manifest.mjs",
    "build:release:npm": "node scripts/build-npm-package.mjs",
    "build:release": "npm run build:release:binaries && npm run build:release:npm",
    "build:release:linux": "npm run build:release:binaries:linux && npm run build:release:npm",
    "pack:release": "node scripts/pack-release.mjs",
    "publish:release": "node scripts/publish-release.mjs",
    "publish:release:dry-run": "node scripts/publish-release.mjs --dry-run",
    "publish:release:single": "node scripts/publish-release.mjs --single",
    "publish:release:single:dry-run": "node scripts/publish-release.mjs --single --dry-run",
    "publish:release:bun": "node scripts/publish-release.mjs --pm bun",
    "publish:release:bun:dry-run": "node scripts/publish-release.mjs --pm bun --dry-run",
    "publish:release:bun:single": "node scripts/publish-release.mjs --pm bun --single",
    "publish:release:bun:single:dry-run": "node scripts/publish-release.mjs --pm bun --single --dry-run",
    "publish:release:bun:linux": "node scripts/publish-release.mjs --pm bun --profile linux",
    "publish:release:bun:linux:dry-run": "node scripts/publish-release.mjs --pm bun --profile linux --dry-run",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:e2e:tmux": "MUDCODE_E2E_TMUX=1 vitest run tests/tmux/manager.integration.test.ts",
    "test:replay:capture": "vitest run tests/bridge/capture-poller.replay.test.ts",
    "release:verify:bun": "npm run typecheck && npm test && npm run test:e2e:tmux && npm run publish:release:bun:dry-run",
    "release:verify:bun:single": "npm run typecheck && npm test && npm run test:e2e:tmux && npm run publish:release:bun:single:dry-run",
    "release:verify:bun:linux": "npm run typecheck && npm test && npm run test:e2e:tmux && npm run publish:release:bun:linux:dry-run",
    "release:publish:bun": "npm run release:verify:bun && npm run publish:release:bun -- --skip-build",
    "release:publish:bun:single": "npm run release:verify:bun:single && npm run publish:release:bun:single -- --skip-build",
    "release:publish:bun:linux": "npm run release:verify:bun:linux && npm run publish:release:bun:linux -- --skip-build",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "ci:local": "node scripts/ci-local.mjs",
    "ci:local:e2e": "node scripts/ci-local.mjs --with-e2e"
  },
  "keywords": [
    "discord",
    "ai-agent",
    "claude-code",
    "tmux",
    "bridge"
  ],
  "author": "",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "optionalDependencies": {
    "@mudramo/mudcode-darwin-arm64": "1.1.3",
    "@mudramo/mudcode-darwin-x64": "1.1.3",
    "@mudramo/mudcode-darwin-x64-baseline": "1.1.3",
    "@mudramo/mudcode-linux-arm64": "1.1.3",
    "@mudramo/mudcode-linux-arm64-musl": "1.1.3",
    "@mudramo/mudcode-linux-x64": "1.1.3",
    "@mudramo/mudcode-linux-x64-baseline": "1.1.3",
    "@mudramo/mudcode-linux-x64-baseline-musl": "1.1.3",
    "@mudramo/mudcode-linux-x64-musl": "1.1.3",
    "@mudramo/mudcode-windows-x64": "1.1.3",
    "@mudramo/mudcode-windows-x64-baseline": "1.1.3"
  },
  "engines": {
    "bun": ">=1.3.0"
  },
  "dependencies": {
    "@opentui/core": "^0.1.77",
    "@opentui/solid": "^0.1.77",
    "@slack/bolt": "^4.6.0",
    "@types/yargs": "^17.0.35",
    "chalk": "^5.3.0",
    "discord.js": "^14.14.1",
    "dotenv": "^16.4.5",
    "solid-js": "^1.9.9",
    "yargs": "^18.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.5",
    "tsup": "^8.0.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vite": "^7.1.5",
    "vitest": "^4.0.18"
  }
}
